{
    "compilerOptions": {
        "module": "commonjs",
        "moduleResolution": "node",
        "lib": [
        "esnext"
        ],
        "preserveConstEnums": true,
        "noImplicitAny": true,
        "noImplicitReturns": true,
        "noImplicitThis": true,
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "noEmitOnError": true,
        "noFallthroughCasesInSwitch": true,
        "strict": true,
        "target": "es2019",
        "sourceMap": false,
        "esModuleInterop": true,
        "resolveJsonModule": true,
        "outDir": "test-temp",
    },
    "include": [
        "test/**.ts",
    ],
}